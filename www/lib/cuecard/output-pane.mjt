<acre:script>
  var templates = this.exports;
  var outputPane;
</acre:script>
 
<acre:block def="tabs(id, oP)">

  <acre:script>
    outputPane = oP;
  </acre:script>
  
  <div id="$id" class="cuecard-outputPane">
    <div class="cuecard-outputPane-tabs">
      <ul class="tab-nav clear">
        <li acre:for="index, tab in outputPane._TABS" class="tab">
          <a href="#${id}-${tab.key}"><span>$tab.name</span></a>
        </li>
      </ul>
    </div>
    <div class="cuecard-outputPane-content tabbed-content module-section">
      <div acre:for="tab in outputPane._TABS" class="cuecard-outputPane-tabBody" id="${id}-${tab.key}">
        <div class="cuecard-outputPane-$tab.key" >
          ${templates[tab.key]()}
        </div>
      </div>
    </div>
  </div>
  
  <acre:block script="ondomready">
    var el = $(outputPane._elmt);
    outputPane._container = el.find(".cuecard-outputPane");
    outputPane._tabsContainer = el.find(".cuecard-outputPane-tabs");
    outputPane._contentContainer = el.find(".cuecard-outputPane-content");
  </acre:block>
</acre:block>


<acre:block def="list(o, c)">
  <table class="data-table" acre:if="o">
    <acre:script>
      function is_constraint(path, value) {
        
        function same_keys(o1, o2) {
          for (var key in o1) {
            var val1 = o1[key];
            var val2 = o2[key];
            val1 = jQuery.isArray(val1) ? val1[0] : val1;
            val2 = jQuery.isArray(val2) ? val2[0] : val2;
            if (val2 === undefined) return false;
            if (jQuery.isPlainObject(val1) && jQuery.isPlainObject(val2)) {
              if (!same_keys(val1, val2)) return false;
            }
          }
          return true;
        }
        
        var val = jQuery.extend({}, c, true);
        value = jQuery.isArray(value) ? value[0] : value;
        for (var s = 0; s < path.length; s++) {
          val = val[path[s]];
          if (val === undefined) return false;
        }
        return jQuery.isPlainObject(val) ? same_keys(value, val) : true;
      }
    </acre:script>
    
    <thead acre:def="thead(r, path)">
      <tr>
        <th acre:for="key, val in r" acre:if="!is_constraint(path.concat(key), val)">
          ${key.split("/").slice(-1).join("")} ${thead_value(val, path.concat(key))}
        </th>
      </tr>
    </thead>
    
    <acre:block def="thead_value(v, path)">
      <acre:script>if (jQuery.isArray(v)) v = v[0];</acre:script>
      <ul acre:if="jQuery.isPlainObject(v)">
        <li acre:for="key, val in v" acre:if="!is_constraint(path.concat(key), val)">
          ${key.split("/").slice(-1).join("")} ${thead_value(val, path.concat(key))}
        </li>
      </ul>
    </acre:block>
    
    <acre:block def="row(r, path)">
      <acre:block if="jQuery.isPlainObject(r)">
        <td acre:for="key, val in r" acre:if="!is_constraint(path.concat(key), val)">
          ${value(val, path.concat(key))}
        </td>
      </acre:block>
      <td acre:else="">${value(val, path)}</td>
    </acre:block>
    
    <acre:block def="value(a, path)">
      <acre:block choose="typeof a">
        <acre:block when="string">$a</acre:block>
        <acre:block when="number">$a</acre:block>
        <acre:block when="boolean">${a ? "true" : "false"}</acre:block>
        <acre:block when="object">
          <ul class="data-list" acre:if="jQuery.isArray(a)">
            <li acre:for="val in a">
              ${value(val, path)}
            </li>
          </ul>
          <ul class="data-list" acre:elif="jQuery.isPlainObject(a)">
            <li acre:for="key, val in a" acre:if="!is_constraint(path.concat(key), val)">
              ${value(val, path.concat(key))}
            </li>
          </ul>
        </acre:block>
      </acre:block>
    </acre:block>
    
    <acre:block if="jQuery.isArray(o)">
      ${thead(o[0], [])}
      <tbody>
        <tr acre:for="a in o">${row(a, [])}</tr>
      </tbody>
    </acre:block>
    <acre:block else="">
      <acre:block if="typeof o === 'object'">${thead(o, [])}</acre:block>
      <tbody>
        <tr>${row(o, [])}</tr>
      </tbody>
    </acre:block>
    
  </table>
</acre:block>


<acre:block def="json()">
  <div class="panel-content"></div>
  <div class="panel-header">
    <span class="cuecard-outputPane-headers drawer-toggle">
      <a class="icon-link" href="javascript:void(0)">
        <span>Costs &amp; Headers</span>
      </a>
    </span>
  </div>
  <div class="drawer-content"></div>
</acre:block>


<acre:block def="status(headers)">
  <div acre:if="headers" class="cuecard-outputPane-responseHeaders">

    <acre:block if="headers['x-metaweb-tid']">
      <acre:script>var tid = headers["x-metaweb-tid"];</acre:script>
      <h3>x-metaweb-tid (transaction ID)</h3>
      <div>
        <a target="_blank" href="http://stats.metaweb.com/query/transaction?tid=${encodeURIComponent(tid)}">
          $tid
        </a>
      </div>
    </acre:block>

    <acre:block if="headers['x-metaweb-cost']">
      <acre:script>
        var xmc = headers["x-metaweb-cost"].split(",");
        var odd = true;
      </acre:script>
      <h3>x-metaweb-cost header components</h3>
      <table>
        <tr><th>code</th><th>value</th><th>meaning</th><th>subsystem</th></tr>
        <acre:block for="index, entry in xmc">
          <acre:script>
            var pair = entry.split("=");
            pair[0] = pair[0].replace(/^\s+/, '').replace(/\s+$/, '');
            var cost = CueCard.XMetawebCosts[pair[0]];
            var rclass = (index % 2 ? "cuecard-outputPane-odd" : "cuecard-outputPane-even") + ((cost !== undefined && "important" in cost && cost.important) ? " cuecard-outputPane-cost-important" : "")
          </acre:script>
          <tr class="$rclass">
            <td>${pair[0]}</td>
            <td>${pair[1]}</td>
            <td acre:if="cost !== undefined">$cost.meaning</td><td acre:else="">--</td>
            <td acre:if="cost !== undefined">$cost.subsystem</td><td acre:else="">--</td>
          </tr>
        </acre:block>
      </table>
    </acre:block>

    <h3>response headers</h3>
    <table>
      <acre:script>var odd = true;</acre:script>
      <acre:block for="key, value in headers" if="key != 'x-metaweb-cost' && key != 'x-metaweb-tid'">
        <tr acre:attrs="odd ? {'class': 'cuecard-outputPane-odd'} : {'class': 'cuecard-outputPane-even'}">
          <td>$key</td>
          <td>$value</td>
        </tr>
        <acre:script>odd = !odd</acre:script>
      </acre:block>
    </table>

  </div>
</acre:block>


<acre:block def="help()">
  <div class="cuecard-outputPane-helpContent">
    <ul>
      <li><a href="http://www.freebase.com/docs/data" target="_blank">MQL Tutorial</a></li>
      <li><a href="http://www.freebase.com/docs/mql" target="_blank">MQL&nbsp;Reference</a></li>
    </ul>

    <div class="modal-inner">
      <h1 class="modal-title">
        <span>Examples</span>
      </h1>
      Techniques:
      <select class="cuecard-examples-technique">
        <option acre:for="technique in CueCard.ExampleTechniques" 
          acre:attrs="(technique === 'all')?{checked:'checked'}:{}" data-technique="$technique">
          $technique (${CueCard.ExampleTechniqueMap[technique].length})
        </option>
      </select>
      
      <acre:block def="list_examples(technique)">
        <a acre:for="index, example in CueCard.Examples" 
           acre:if="((technique !== 'all') ? (example.techniques.indexOf(technique) !== -1) : true)"
           href="javascript:selectExample('$index')" class="cuecard-examples-example" id="cuecard-example-$index">
          $example.name
        </a>
      </acre:block>
      
      <div class="cuecard-examples-list"></div>
    </div>
    <acre:block script="ondomready">
      selectTechnique = function(technique) {
        technique = technique || 'all';
        $(".cuecard-examples-list").acre(list_examples(technique));
        return false;
      }

      selectExample = function(index) {
        $(".cuecard-examples-example").removeClass("cuecard-examples-example-selected");
        $("#cuecard-example-" + index).addClass("cuecard-examples-example-selected");
        outputPane._options.queryEditor.content(CueCard.jsonize(CueCard.Examples[index].query, { indentCount: 2 }));
        return false;
      }

      selectTechnique();
      
      $(".cuecard-examples-technique").change(function(){
        var technique = $("option:selected", this).attr("data-technique");
        selectTechnique(technique);
      });
    </acre:block>
  </div>
</acre:block>
