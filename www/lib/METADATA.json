{
  "ttl": 0,
  "error_page" : "error/error.mjt",
  "mounts": {
    "ajax": "routing/ajax.sjs",
    "static": "routing/static.sjs",
    "libraries": "//2.libraries.apps.freebase.dev",
    "service": "//service"
  },
  "handlers": {
    "mjt": "handlers/mjt_handler.sjs",
    "css": "handlers/css_handler.sjs",
    "css_manifest": "handlers/css_manifest_handler.sjs",
    "js_manifest": "handlers/js_manifest_handler.sjs",
    "tagged_static": "handlers/tagged_static_handler.sjs",
    "tagged_binary": "handlers/tagged_binary_handler.sjs", 
    "template": "handlers/template_handler.sjs",
    "controller": "handlers/controller_handler.sjs",
    "ajax": "handlers/ajax_handler.sjs"
  },
  "extensions": {
    "ico" : { 
      "handler": "tagged_binary",
      "media_type" : "image/x-icon"
    },
    "css": {
      "handler": "css"
    },
    "less": {
      "handler": "css",
      "media_type": "text/css"
    },
    "mf.css": {
      "handler": "css_manifest",
      "media_type": "text/css"
    },
    "mf.js": {
      "handler": "js_manifest",
      "media_type": "application/x-javascript"
    },
    "template": {
      "handler": "template"
    },
    "controller": {
      "handler": "controller"
    },
    "ajax": {
      "handler": "ajax"
    },
    "omf.js": {
      "media_type": "text/javascript", 
      "handler": "js_manifest"
    }, 
    "omf.css": {
      "media_type": "text/css", 
      "handler": "css_manifest"
    },
    "properties": {
      "handler": "acre_script"
    }             
  },
  "static_base": "",
  "libs" : {
    "suggest" : {
      "base_url": "http://freebaselibs.com/static/suggest/",
      "version": "1.4.1"
    },
    "jquery" : {
      "base_url": "http://ajax.googleapis.com/ajax/libs/jquery/",
      "version": "1.5.1"
    }
  }
}
