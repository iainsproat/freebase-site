<acre:script>

  var mf = acre.require("MANIFEST").MF;
  var id = acre.request.params['id'];
  var app_util = mf.require('app_util');
  
  var result = app_util.get_app(id);
  var context = app_util.deconstruct_id(id);
  var app = result[result.reference_env || 'production'];
  
  app['manifest'] = app_util.get_manifest_contents(id, app);
  //console.log(app);
</acre:script>

<div id="reference">
 <i>${app['relative_date']}</i>
 <div class="manifest" acre:if="app['manifest']">
    <br/><b>manifest</b>
    <acre:block for="app_id, version in app['manifest']['version']">
      <br/>${app_id}: ${version || 'trunk'}
    </acre:block>
  </div>
</div>


