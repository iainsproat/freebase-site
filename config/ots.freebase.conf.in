[
    {
        "host": "@ACRE_HOST_BASE@",
        "rules": [
            {
                "from": {
                    "path": "/" 
                },
                "to": {
                    "route_as": "app",
                    "location": "/freebase/site/apps/release"
                } 
            }
        ]
    },
    {
        "host": ".@ACRE_METAWEB_BASE_ADDR@",
        "rules": [
            {
                "from":{
                    "path":"/api"
                },
                "to":{
                    "route_as":"servlet",
                    "servlet":"ProxyPassServlet"
                }
            },
            {
                "from": {
                    "path": "/" 
                },
                "to": {
                    "route_as": "app",
                    "location": "/freebase/site/routing/release" 
                } 
            }
        ]
    }
]